    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.8.4"
  }
